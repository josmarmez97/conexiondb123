@{
    ViewData["Title"] = "Index";

}
@model List<postgres.Models.Usuarios>
<div>
    <h2>INDEX</h2>
    <a asp-controller="Registro" asp-action="Altas">Registrar</a>
</div>

<div>
    <h4>Cuentas registradas</h3>
    <table style="width:100%">
            <tr>
                <th>id</th>
                <th>userid</th>
                <th>nombre</th>
                <th>pass</th>
            </tr>
            @foreach(var item in Model)
            {
                <tr>
                    <td>@item.IdUsuarios</td>
                     <td>@item.Userid </td>
                    <td> @item.Nombre </td>
                     <td>@item.Pass </td>
                    <td><a href="#" class="btn btn-danger">Eliminar</a>
                    <a href="#" class="btn btn-success">Detalles</a></td>
                    
                </tr>
            }
    </table>

</div>